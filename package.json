{
  "name": "package-diff-summary",
  "version": "2.0.0",
  "description": "compare package.json files to create CHANGELOG.md entries",
  "keywords": [
    "change log",
    "changelog",
    "compare",
    "diff",
    "git",
    "package",
    "package.json",
    "summary"
  ],
  "homepage": "https://github.com/jokeyrhyme/package-diff-summary.js#readme",
  "bugs": {
    "url": "https://github.com/jokeyrhyme/package-diff-summary.js/issues"
  },
  "license": "MIT",
  "author": "Ron Waldon <jokeyrhyme@gmail.com> (https://github.com/jokeyrhyme)",
  "files": [
    "bin",
    "lib",
    "index.js"
  ],
  "main": "index.js",
  "bin": {
    "package-diff-summary": "bin/index.js"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jokeyrhyme/package-diff-summary.js.git"
  },
  "scripts": {
    "eslint": "eslint --fix --cache .",
    "flow": "npx -q flow-bin@^0.54.1 check",
    "jest": "jest",
    "nsp": "npx -q nsp check",
    "sort-package-json": "npx -q sort-package-json",
    "pretest": "npm run prettier && npm run eslint && npm run sort-package-json",
    "test": "npm run jest && npm run nsp && npm run flow",
    "prettier": "npx -q prettier --list-different --write \"{,!(build|coverage|dist|flow-typed|typings|vendor)/**/}*.{css,js,jsx,less,scss,ts,tsx}\" || exit 0"
  },
  "dependencies": {
    "dependency-diff": "^1.0.4",
    "execa": "^0.8.0",
    "github": "^11.0.0",
    "github-url-from-git": "^1.5.0",
    "got": "^7.0.0",
    "meow": "^3.7.0",
    "package-engines-notifier": "^1.1.0",
    "read-pkg-up": "^2.0.0",
    "semver": "^5.3.0",
    "update-nodejs-notifier": "^1.1.0",
    "update-notifier": "2.5.0"
  },
  "devDependencies": {
    "eslint": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "jest": "^21.0.2",
    "npx": "^9.6.0"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=4"
  },
  "eslintIgnore": [
    "coverage",
    "flow-typed"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "lines": 90
      }
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
