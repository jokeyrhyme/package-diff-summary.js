{
  "name": "package-diff-summary",
  "description": "compare package.json files to create CHANGELOG.md entries",
  "version": "1.3.0",
  "author": "Ron Waldon <jokeyrhyme@gmail.com> (https://github.com/jokeyrhyme)",
  "bin": {
    "package-diff-summary": "bin/index.js"
  },
  "bugs": {
    "url": "https://github.com/jokeyrhyme/package-diff-summary.js/issues"
  },
  "dependencies": {
    "dependency-diff": "^1.0.4",
    "execa": "^0.6.0",
    "github": "^9.0.0",
    "github-url-from-git": "^1.5.0",
    "got": "^6.7.1",
    "meow": "^3.7.0",
    "package-engines-notifier": "^1.1.0",
    "read-pkg-up": "^2.0.0",
    "semver": "^5.3.0",
    "update-nodejs-notifier": "^1.1.0",
    "update-notifier": "^2.1.0"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.0",
    "fixpack": "^2.3.1",
    "flow-bin": "^0.43.0",
    "jest": "^19.0.0"
  },
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=4"
  },
  "files": [
    "bin",
    "lib",
    "index.js"
  ],
  "homepage": "https://github.com/jokeyrhyme/package-diff-summary.js#readme",
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "lines": 90
      }
    }
  },
  "keywords": [
    "change log",
    "changelog",
    "compare",
    "diff",
    "git",
    "package",
    "package.json",
    "summary"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jokeyrhyme/package-diff-summary.js.git"
  },
  "scripts": {
    "eslint": "eslint --fix --cache .",
    "fixpack": "fixpack",
    "flow_check": "flow check",
    "jest": "jest",
    "posttest": "npm run eslint && npm run flow_check",
    "pretest": "npm run fixpack",
    "test": "npm run jest"
  }
}
